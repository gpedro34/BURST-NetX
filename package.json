{
  "name": "burst-netx",
  "version": "0.0.1",
  "description": "Network Explorer API and front-end",
  "main": "server.js",
  "keywords": [
    "burst",
    "brs",
    "network",
    "crawler"
  ],
  "scripts": {
    "server": "node server/server.js",
    "client": "npm start --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\" --kill-others",
    "setup": "npm install && cd client && npm install && cd .. && npm start",
    "clean-setup": "bash ./clean-setup.sh"
  },
  "author": "gpedro",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "iplocation": "^6.1.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.4",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "ssl-checker": "^1.2.1"
  }
}
